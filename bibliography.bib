@article{stellatoRoomtemperatureMacromolecularSerial2014,
  title = {Room-Temperature Macromolecular Serial Crystallography Using Synchrotron Radiation},
  author = {Stellato, Francesco and Oberth{\"u}r, Dominik and Liang, Mengning and Bean, Richard and Gati, Cornelius and Yefanov, Oleksandr and Barty, Anton and Burkhardt, Anja and Fischer, Pontus and Galli, Lorenzo and Kirian, Richard A. and Meyer, Jan and Panneerselvam, Saravanan and Yoon, Chun Hong and Chervinskii, Fedor and Speller, Emily and White, Thomas A. and Betzel, Christian and Meents, Alke and Chapman, Henry N.},
  year = {2014},
  month = jul,
  journal = {IUCrJ},
  volume = {1},
  number = {4},
  pages = {204--212},
  issn = {2052-2525},
  doi = {10.1107/S2052252514010070},
  abstract = {A new approach for collecting data from many hundreds of thousands of microcrystals using X-ray pulses from a free-electron laser has recently been developed. Referred to as serial crystallography, diffraction patterns are recorded at a constant rate as a suspension of protein crystals flows across the path of an X-ray beam. Events that by chance contain single-crystal diffraction patterns are retained, then indexed and merged to form a three-dimensional set of reflection intensities for structure determination. This approach relies upon several innovations: an intense X-ray beam; a fast detector system; a means to rapidly flow a suspension of crystals across the X-ray beam; and the computational infrastructure to process the large volume of data. Originally conceived for radiation-damage-free measurements with ultrafast X-ray pulses, the same methods can be employed with synchrotron radiation. As in powder diffraction, the averaging of thousands of observations per Bragg peak may improve the ratio of signal to noise of low-dose exposures. Here, it is shown that this paradigm can be implemented for room-temperature data collection using synchrotron radiation and exposure times of less than 3\hspace{0.25em}ms. Using lysozyme microcrystals as a model system, over 40\,000 single-crystal diffraction patterns were obtained and merged to produce a structural model that could be refined to 2.1\hspace{0.25em}\AA{} resolution. The resulting electron density is in excellent agreement with that obtained using standard X-ray data collection techniques. With further improvements the method is well suited for even shorter exposures at future and upgraded synchrotron radiation facilities that may deliver beams with 1000 times higher brightness than they currently produce.},
  langid = {english},
  file = {/Users/cwang31/Library/CloudStorage/OneDrive-SLACNationalAcceleratorLaboratory/ZotPDF/Stellato et al_2014_Room-temperature macromolecular serial crystallography using synchrotron.pdf}
}

