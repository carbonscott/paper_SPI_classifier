@article{parkFixedTargetSerialSynchrotron2020,
  title = {Fixed-{{Target Serial Synchrotron Crystallography Using Nylon Mesh}} and {{Enclosed Film-Based Sample Holder}}},
  author = {Park, Suk-Youl and Choi, Hyeongju and Eo, Cheolsoo and Cho, Yunje and Nam, Ki Hyun},
  year = {2020},
  month = sep,
  journal = {Crystals},
  volume = {10},
  number = {9},
  pages = {803},
  issn = {2073-4352},
  doi = {10.3390/cryst10090803},
  abstract = {Serial crystallography (SX) technique using synchrotron X-ray allows the visualization of room-temperature crystal structures with low-dose data collection as well as time-resolved molecular dynamics. In an SX experiment, delivery of numerous crystals for X-ray interaction, in a serial manner, is very important. Fixed-target scanning approach has the advantage of dramatically minimizing sample consumption as well as any physical damage to crystal sample, compared to other sample delivery methods. Here, we introduce the simple approach of fixed-target serial synchrotron crystallography (FT-SSX) using nylon mesh and enclosed film (NAM)-based sample holder. The NAM-based sample holder consisted of X-ray-transparent nylon-mesh and polyimide film, attached to a magnetic base. This sample holder was mounted to a goniometer head on macromolecular crystallography beamline, and translated along vertical and horizontal directions for raster scanning by the goniometer. Diffraction data were collected in two raster scanning approaches: (i) 100 ms X-ray exposure and 0.011\textdegree{} oscillation at each scan point and (ii) 500 ms X-ray exposure and 0.222\textdegree{} oscillation at each scan point. Using this approach, we determined the room-temperature crystal structures of lysozyme and glucose isomerase at 1.5\textendash 2.0 \AA{} resolution. The sample holder produced negligible X-ray background scattering for data processing. Therefore, the new approach provided an opportunity to perform FT-SSX with high accessibility using macromolecular crystallography beamlines at synchrotron without any special equipment.},
  langid = {english},
  file = {/Users/cwang31/Library/CloudStorage/OneDrive-SLACNationalAcceleratorLaboratory/ZotPDF/Park et al_2020_Fixed-Target Serial Synchrotron Crystallography Using Nylon Mesh and Enclosed.pdf}
}

