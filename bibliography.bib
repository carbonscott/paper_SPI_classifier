@article{kockAdvancedFloatingChirality2020,
  title = {The {{Advanced Floating Chirality Distance Geometry Approach}}\rule{1em}{1pt}{{How Anisotropic NMR Parameters Can Support}} the {{Determination}} of the {{Relative Configuration}} of {{Natural Products}}},
  author = {K{\"o}ck, Matthias and Reggelin, Michael and Immel, Stefan},
