  issn = {14337851},
  doi = {10.1002/anie.201810266},
  abstract = {The origin of the spectral shift from a red- to a greenabsorbing form in a cyanobacteriochrome, Slr1393g3, was identified by combined quantum mechanics/molecular mechanics simulations. This protein, related to classical phytochromes, carries the open-chain tetrapyrrole chromophore phycocyanobilin. Our calculations reveal that the effective conjugation length in the chromophore becomes shorter upon conversion from the red to the green form. This is related to the planarity of the entire chromophore. A large distortion was found for the terminal pyrrole rings A and D; however, the D ring contributes more strongly to the photoproduct tuning, despite a larger change in the twist of the A ring. Our findings implicate that the D ring twist can be exploited to regulate the absorption of the photoproduct. Hence, mutations that affect the D ring twist can lead to rational tuning of the photoproduct absorption, allowing the tailoring of cyanobacteriochromes for biotechnological applications such as optogenetics and bioimaging.},
  langid = {english},
  file = {/Users/scott/Dropbox/ZotPDF/Wiebeler et al_2019_The Effective Conjugation Length Is Responsible for the Red-Green Spectral.pdf}
}

