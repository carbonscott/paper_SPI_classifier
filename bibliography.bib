@article{wangInsightsBaseSelectivity2011,
  title = {Insights into {{Base Selectivity}} from the 1.8 {{\AA{} Resolution Structure}} of an {{RB69 DNA Polymerase Ternary Complex}}},
  author = {Wang, Mina and Xia, Shuangluo and Blaha, Gregor and Steitz, Thomas A. and Konigsberg, William H. and Wang, Jimin},
  year = {2011},
  month = feb,
  journal = {Biochemistry},
  volume = {50},
  number = {4},
  pages = {581--590},
  issn = {0006-2960, 1520-4995},
  doi = {10.1021/bi101192f},
  abstract = {Bacteriophage RB69 DNA polymerase (RB69 pol) has served as a model for investigating how B family polymerases achieve a high level of fidelity during DNA replication. We report here the structure of an RB69 pol ternary complex at 1.8 A\r{} resolution, extending the resolution from our previously reported structure at 2.6 A\r{} [Franklin, M. C., et al. (2001) Cell 105, 657-667]. In the structure presented here, a network of five highly ordered, buried water molecules can be seen to interact with the N3 and O2 atoms in the minor groove of the DNA duplex. This structure reveals how the formation of the closed ternary complex eliminates two ordered water molecules, which are responsible for a kink in helix P in the apo structure. In addition, three pairs of polar-nonpolar interactions have been observed between (i) the CR hydrogen of G568 and the N3 atom of the dG templating base, (ii) the O50 and C5 atoms of the incoming dCTP, and (iii) the OH group of S565 and the aromatic face of the dG templating base. These interactions are optimized in the dehydrated environment that envelops Watson-Crick nascent base pairs and serve to enhance base selectivity in wildtype RB69 pol.},
  langid = {english},
  file = {/Users/cwang31/Library/CloudStorage/OneDrive-SLACNationalAcceleratorLaboratory/ZotPDF/Wang et al_2011_Insights into Base Selectivity from the 1.pdf}
}

