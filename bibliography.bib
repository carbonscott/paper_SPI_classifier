@article{jacob-dubuissonStructuralInsightsSignalling2018,
  title = {Structural Insights into the Signalling Mechanisms of Two-Component Systems},
  author = {{Jacob-Dubuisson}, Fran{\c c}oise and Mechaly, Ariel and Betton, Jean-Michel and Antoine, Rudy},
  year = {2018},
