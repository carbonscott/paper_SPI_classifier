@article{srajerProteinConformationalRelaxation2001,
  title = {Protein {{Conformational Relaxation}} and {{Ligand Migration}} in {{Myoglobin}}: {{A Nanosecond}} to {{Millisecond Molecular Movie}} from {{Time-Resolved Laue X-ray Diffraction}} {\textsuperscript{\textdagger}}},
  shorttitle = {Protein {{Conformational Relaxation}} and {{Ligand Migration}} in {{Myoglobin}}},
  author = {{\v S}rajer, Vukica and Ren, Zhong and Teng, Tsu-Yi and Schmidt, Marius and Ursby, Thomas and Bourgeois, Dominique and Pradervand, Claude and Schildkamp, Wilfried and Wulff, Michael and Moffat, Keith},
  year = {2001},
  month = nov,
  journal = {Biochemistry},
  volume = {40},
  number = {46},
  pages = {13802--13815},
  issn = {0006-2960, 1520-4995},
  doi = {10.1021/bi010715u},
  abstract = {A time-resolved Laue X-ray diffraction technique has been used to explore protein relaxation and ligand migration at room temperature following photolysis of a single crystal of carbon monoxymyoglobin. The CO ligand is photodissociated by a 7.5 ns laser pulse, and the subsequent structural changes are probed by 150 ps or 1 \textmu s X-ray pulses at 14 laser/X-ray delay times, ranging from 1 ns to 1.9 ms. Very fast heme and protein relaxation involving the E and F helices is evident from the data at a 1 ns time delay. The photodissociated CO molecules are detected at two locations: at a distal pocket docking site and at the Xe 1 binding site in the proximal pocket. The population by CO of the primary, distal site peaks at a 1 ns time delay and decays to half the peak value in 70 ns. The secondary, proximal docking site reaches its highest occupancy of 20\% at {$\sim$}100 ns and has a half-life of {$\sim$}10 \textmu s. At {$\sim$}100 ns, all CO molecules are accounted for within the protein: in one of these two docking sites or bound to the heme. Thereafter, the CO molecules migrate to the solvent from which they rebind to deoxymyoglobin in a bimolecular process with a second-order rate coefficient of 4.5 \texttimes{} 105 M-1 s-1. Our results also demonstrate that structural changes as small as 0.2 \AA{} and populations of CO docking sites of 10\% can be detected by time-resolved X-ray diffraction.},
  langid = {english},
  file = {/Users/scott/Dropbox/ZotPDF/SI-Šrajer et al_2001_Protein Conformational Relaxation and Ligand Migration in Myoglobin.pdf;/Users/scott/Dropbox/ZotPDF/Šrajer et al_2001_Protein Conformational Relaxation and Ligand Migration in Myoglobin.pdf}
}

@article{srajerWatchingProteinsFunction2017,
  title = {Watching Proteins Function with Time-Resolved x-Ray Crystallography},
  author = {{\v S}rajer, Vukica and Schmidt, Marius},
  year = {2017},
  month = sep,
  journal = {Journal of Physics D: Applied Physics},
  volume = {50},
  number = {37},
  pages = {373001},
  issn = {0022-3727, 1361-6463},
  doi = {10.1088/1361-6463/aa7d32},
  abstract = {Macromolecular crystallography was immensely successful in the last two decades. To a large degree this success resulted from use of powerful third generation synchrotron x-ray sources. An expansive database of more than 100\,000 protein structures, of which many were determined at resolution better than 2 \AA, is available today. With this achievement, the spotlight in structural biology is shifting from determination of static structures to elucidating dynamic aspects of protein function. A powerful tool for addressing these aspects is timeresolved crystallography, where a genuine biological function is triggered in the crystal with a goal of capturing molecules in action and determining protein kinetics and structures of intermediates (Schmidt et al 2005a Methods Mol. Biol. 305 115\textendash 54, Schmidt 2008 Ultrashort Laser Pulses in Biology and Medicine (Berlin: Springer) pp 201\textendash 41, Neutze and Moffat 2012 Curr. Opin. Struct. Biol. 22 651\textendash 9, \v{S}rajer 2014 The Future of Dynamic Structural Science (Berlin: Springer) pp 237\textendash 51). In this approach, short and intense x-ray pulses are used to probe intermediates in real time and at room temperature, in an ongoing reaction that is initiated synchronously and rapidly in the crystal. Time-resolved macromolecular crystallography with 100\,ps time resolution at synchrotron x-ray sources is in its mature phase today, particularly for studies of reversible, light-initiated reactions. The advent of the new free electron lasers for hard x-rays (XFELs; 5\textendash 20\,keV), which provide exceptionally intense, femtosecond x-ray pulses, marks a new frontier for time-resolved crystallography. The exploration of ultra-fast events becomes possible in high-resolution structural detail, on sub-picosecond time scales (Tenboer et al 2014 Science 346 1242\textendash 6, Barends et al 2015 Science 350 445\textendash 50, Pande et al 2016 Science 352 725\textendash 9). We review here state-of-the-art time-resolved crystallographic experiments both at synchrotrons and XFELs. We also outline challenges and further developments necessary to broaden the application of these methods to many important proteins and enzymes of biomedical relevance.},
  langid = {english},
  file = {/Users/cwang31/Library/CloudStorage/OneDrive-SLACNationalAcceleratorLaboratory/ZotPDF/Šrajer_Schmidt_2017_Watching proteins function with time-resolved x-ray crystallography.pdf}
}

