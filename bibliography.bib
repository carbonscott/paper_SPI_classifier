@article{gatiSerialCrystallographyVivo2014,
  title = {Serial Crystallography on {\emph{in Vivo}} Grown Microcrystals Using Synchrotron Radiation},
  author = {Gati, Cornelius and Bourenkov, Gleb and Klinge, Marco and Rehders, Dirk and Stellato, Francesco and Oberth{\"u}r, Dominik and Yefanov, Oleksandr and Sommer, Benjamin P. and Mogk, Stefan and Duszenko, Michael and Betzel, Christian and Schneider, Thomas R. and Chapman, Henry N. and Redecke, Lars},
