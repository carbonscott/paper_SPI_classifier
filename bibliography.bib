@article{mattleLigandChannelPharmacologically2018,
  title = {Ligand Channel in Pharmacologically Stabilized Rhodopsin},
  author = {Mattle, Daniel and Kuhn, Bernd and Aebi, Johannes and Bedoucha, Marc and Kekilli, Demet and Grozinger, Nathalie and Alker, Andre and Rudolph, Markus G. and Schmid, Georg and Schertler, Gebhard F. X. and Hennig, Michael and Standfuss, J{\"o}rg and Dawson, Roger J. P.},
