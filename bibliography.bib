@article{carpentierProteinMultipleAlignments2019,
  title = {Protein Multiple Alignments: Sequence-Based versus Structure-Based Programs},
  shorttitle = {Protein Multiple Alignments},
  author = {Carpentier, Mathilde and Chomilier, Jacques},
  editor = {Valencia, Alfonso},
  year = {2019},
